{
  "name": "production-instance",
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "default_attributes": {
    "php": {
      "directives": {
        "date.timezone": "UTC",
        "post_max_size": "32M",
        "apc.shm_size": "256M"
      },
      "opcache-directives": {
        "opcache.memory_consumption": "256M",
        "opcache.validate_timestamps": 0
      }
    }
  }
}
